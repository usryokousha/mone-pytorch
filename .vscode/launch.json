    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: train.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/mone_pytorch/train/train.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${env:PYTHONPATH}"
            }
        },
        {
            "name": "Python Debugger: routing.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/mone_pytorch/layers/routing.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${env:PYTHONPATH}"
            }
        }
    ]
}